{{form_start(form,{attr: { novalidate : 'novalidate' }})  }}
<section>
    
<div class="container">
                                        <form class="form-horizontal" role="form">

                                           
                                            <div class="form-group">
                                                <label class="col-2 col-form-label">Nom :</label>
                                                <div class="col-10">
                                                    {{ form_widget(form.nom, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.nom) }}

                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-2 col-form-label" >Prenom :</label>
                                                <div class="col-10">
                                                    {{ form_widget(form.prenom, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.prenom) }}

                                                </div>
                                            </div>
                                            
                                            <div class="form-group">
                                                <label class="col-2 col-form-label" >email :</label>
                                                <div class="col-10">
                                                    {{ form_widget(form.email, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.email) }}

                                                </div>
                                            </div>
                                            
                                            <div class="form-group">
                                                <label class="col-2 col-form-label" for="example-email">Mot de passe :</label>
                                                <div class="col-10">
                                                    {{ form_widget(form.mdp, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.mdp) }}

                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-2 col-form-label" >Numero de telephone :</label>
                                                <div class="">
                                                    {{ form_widget(form.numtel, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.numtel) }}

                                                </div>
                                            </div>
                                       
                                            
                                            <div class="form-group text-center">
                                            <button  class="btn theme-btn full-width btn-m">{{ button_label|default('Ajouter') }}</button>
                                            <a class="btn btn-primary" href="{{ path('app_admins_index') }}">Retour</a>
                                            </div>
                                            
</div>
</section>
                                            {{ form_end(form) }}