
{{form_start(form,{attr: { novalidate : 'novalidate' }})  }}
<section>
<div class="container">
                                        <form class="form-horizontal" role="form">
<h1>Create your CampersDen Account and join us !</h1>

                                            <div class="form-group">
                                                <label class="col-2 col-form-label">Cin :</label>
                                                <div class="col-10">
                                                    {{ form_widget(form.cin, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.cin) }}

                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-2 col-form-label">Full Name :</label>
                                                <div class="col-10">
                                                    {{ form_widget(form.nomprenom, { 'attr': {'class': 'form-control'} }) }}
