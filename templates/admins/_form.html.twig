{{form_start(form,{attr: { novalidate : 'novalidate' }})  }}
<section>
<div class="container">
    <br><br>
                                            <form class="forms-sample">
                                                <div class="form-group">
                                            <label >Nom :</label>
                                                    {{ form_widget(form.nom, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.nom) }}
                                            </div>

                                            <div class="form-group">
                                                <label>Prenom :</label>
                                                    {{ form_widget(form.prenom, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.prenom) }}
                                                </div>
                                            <div class="form-group">
                                                <label >Email :</label>
                                                    {{ form_widget(form.email, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.email) }}

                                                </div>
                                            
                                            <div class="form-group">
                                                <label for="example-email">Mot de passe :</label>
                                                    {{ form_widget(form.password, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.password) }}

                                                </div>
                                            <div class="form-group">
                                                <label >Numero de telephone :</label>
                                                    {{ form_widget(form.numtel, { 'attr': {'class': 'form-control'} }) }}
                                                    {{ form_errors(form.numtel) }}

                                                </div>
                                       
                                            
                                            <div class="form-group text-center">
                                            <button  class="btn btn-outline-primary btn-icon-text">{{ button_label|default('Ajouter') }}</button>
                                            <a class="btn btn-danger btn-icon-text" href="{{ path('app_admins_index') }}">Retour</a>
                                            </div>
</div>
</section>
                                            {{ form_end(form) }}