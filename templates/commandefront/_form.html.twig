
{{form_start(form,{attr: { novalidate : 'novalidate' }})  }}
<section>
    <div class="container">
        <form class="form-horizontal" role="form">

            <div class="form-group">
                <label class="col-2 col-form-label">Name :</label>
                <div class="col-10">
                    {{ form_widget(form.nom, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.nom) }}

                </div>
            </div>
            <div class="form-group">
                <label class="col-2 col-form-label">Last Name :</label>
                <div class="col-10">
                    {{ form_widget(form.prenom, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.prenom) }}

                </div>
            </div>
            <div class="form-group">
                <label class="col-2 col-form-label" >Phone Number :</label>
                <div class="col-10">
                    {{ form_widget(form.numTel, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.numTel) }}

                </div>
            </div>

            <div class="form-group">
                <label class="col-2 col-form-label" >Code Postal :</label>
                <div class="col-10">
                    {{ form_widget(form.codepostal, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.codepostal) }}

                </div>
            </div>
            <br></br>
            <div class="form-group">
                <label class="col-2 col-form-label" for="example-email">Gender :</label>
                <div class="col-10">
                    {{ form_widget(form.etat, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.etat) }}

                </div>
            </div>
            <br></br>
            <div class="form-group">
                <label class="col-2 col-form-label" >Email :</label>
                <div class="col-10">
                    {{ form_widget(form.adressmail, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.adressmail) }}

                </div>
            </div>
            <div class="form-group">
                <label class="col-2 col-form-label" >Payment Method :</label>
                <div class="col-10">
                    {{ form_widget(form.modePaiment , { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.modePaiment) }}

                </div>
            </div>

            <div class="form-group text-center">
                <button class="btn theme-btn full-width btn-m">{{ button_label|default('Validate') }}</button>
            </div>
            <a class="btn btn-primary" href="{{ path('indexFront') }}">Cancel</a>
    </div>
</section>
{{ form_end(form) }}
