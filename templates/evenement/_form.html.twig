{{ form_start(form, {'attr': { 'novalidate' : 'novalidate'}}) }}
   {# {{ form_widget(form) }}
    <button class="btn">{{ button_label|default('Save') }}</button> #}
<section>
    <div class="container">
        <form class="form-horizontal" role="form">
            <h1>Evenement</h1>


            <div class="form-group">
                <label class="col-2 col-form-label">Nom :</label>
                <div class="col-10">
                    {{ form_widget(form.nom, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.nom) }}

                </div>
            </div>
            <div class="form-group">
                <label class="col-5 col-form-label" >Description :</label>
                <div class="col-10">
                    {{ form_widget(form.description, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.description) }}

                </div>
            </div>

            <br>
            </br>

            <div class="form-group">
                <label class="col-5 col-form-label" > Date de d√©but :</label>
                <div class="col-10">
                    {{ form_widget(form.dateDeb, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.dateDeb) }}

                </div>
            </div>

            <div class="form-group">
                <label class="col-5 col-form-label" >Date fin :</label>
                <div class="col-10">
                    {{ form_widget(form.dateFin, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.dateFin) }}

                </div>
            </div>

            <div class="form-group">
                <label class="col-5 col-form-label" >Image :</label>
                <div class="col-10">
                    {{ form_widget(form.image, { 'attr': {'class': 'form-control'} }) }}
                    {{ form_errors(form.image) }}

                </div>
            </div>
            <div class="form-group text-center">
                <input type="submit" value="Save"   class="btn theme-btn full-width btn-m" >
            </div>
           {# <a class="btn btn-primary" href="{{ path('app_reservation_index') }}">Retour</a>#}
    </div>
</section>
{{ form_end(form) }}
