

{% block body %}
    <h1>Votre Commande</h1>

    <table class="table table-success table-striped">
        <tbody>
            <tr>
                <th>IdCmd</th>
                <td>{{ commande.idCmd }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ commande.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ commande.prenom }}</td>
            </tr>
            <tr>
                <th>NumTel</th>
                <td>{{ commande.numTel }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ commande.codepostal }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ commande.etat }}</td>
            </tr>
            <tr>
                <th>Adressmail</th>
                <td>{{ commande.adressmail }}</td>
            </tr>
            <tr>
                <th>ModePaiment</th>
                <td>{{ commande.modePaiment }}</td>
            </tr>
        </tbody>
    </table>



   
{% endblock %}
